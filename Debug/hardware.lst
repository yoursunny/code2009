                            	F_SP_ReadQueue_A2000:
                            	F_SP_ReadQueue_S480:
                            	F_SP_ReadQueue_S240:
                            	F_SP_ReadQueue_MS01:
                            	F_SP_ReadQueue_DVR:
                            	F_SP_ReadQueue:
0000A986 12 95 A1 06        			r2 = [R_ReadIndex]
0000A988 12 45 A2 06        			cmp r2,[R_WriteIndex]
0000A98A 0D 5E              			je	L_RQ_QueueEmpty
                            	
0000A98B 0A 05 11 06        			r2 += R_Queue				// get queue data address
0000A98D C2 92              			r1 = [r2]
                            			
0000A98E 12 95 A1 06        			r2 = [R_ReadIndex]
0000A990 41 04              			r2 += 1
0000A991 0A 45 90 00        			cmp	r2, C_QueueSize
0000A993 01 4E              			jne	L_RQ_NotQueueBottom
0000A994 40 94              			r2 = 0
                            	L_RQ_NotQueueBottom:	
0000A995 1A D5 A1 06        			[R_ReadIndex] = r2		
                            			//r2 = 0x0000 						// get queue data
0000A997 90 9A              			retf 
                            	L_RQ_QueueEmpty:
                            			//r2 = 0x8000 						// queue empty
0000A998 90 9A              			retf 
                            	
                            	///////////////////////////////////////////
                            	// Function: Get a data from Queue but do 
                            	//			not change queue index
                            	// R1: output
                            	// Destory: R1,R2
