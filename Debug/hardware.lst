                            	////////////////////////////////////////////////////////////////// 
                            	// Function: Get data from resource(ROM area)
                            	// 	int SP_GetResource(int Addr, int Page)
                            	//////////////////////////////////////////////////////////////////  
                            	_SP_GetResource:	.PROC
0000AB40 88 DA              			push bp to [sp] 
0000AB41 08 0B 01 00        			bp = sp + 1 
                            			
0000AB43 03 92              			r1 = [bp+3] 					// Address
0000AB44 04 94              			r2 = [bp+4] 					// Page
                            			
0000AB45 5A 95              			r2 = r2 lsl 4 					// Prepare Page for SR
0000AB46 5A 95              			r2 = r2 lsl 4 
0000AB47 4A 95              			r2 = r2 lsl 2 
                            			
0000AB48 7F BC              			sr &= 0x03f 					// Change Page
0000AB49 06 A5              	        r2 |=sr 						//
0000AB4A 02 9D              	        sr = r2 						//
                            	  
0000AB4B E1 92              	  		r1 = D:[r1] 					// Get data
                            	       
0000AB4C 88 98              			pop	 bp from [sp] 
0000AB4D 90 9A              			retf 
                            			.ENDP 
