                            	////////////////////////////////////////////////////////
                            	// Function: Ramp Up/Down to avoid speaker "pow" noise
                            	// Destory: R1,R2
                            	////////////////////////////////////////////////////////
                            	_SP_RampUpDAC1:	.PROC
                            	F_SP_RampUpDAC1:
0000AA9E 90 D4              			push r1,r2 to [sp] 
0000AA9F 11 93 17 70        	        r1=[P_DAC1] 
0000AAA1 09 B3 C0 FF        	        r1 &= ~0x003f 
0000AAA3 09 43 00 80        	        cmp     r1,0x8000
0000AAA5 0E 0E              	        jb     	L_RU_NormalUp
0000AAA6 19 5E              	        je      L_RU_End
                            	                
                            	L_RU_DownLoop:
0000AAA7 40 F0 0A AB        	        call    F_Delay         
0000AAA9 41 94              	        r2 = 0x0001 
0000AAAA 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000AAAC 09 23 40 00        	        r1 -= 0x40 
0000AAAE 19 D3 17 70        	        [P_DAC1] = r1 
0000AAB0 09 43 00 80        	        cmp     r1,0x8000 
0000AAB2 4C 4E              	        jne     L_RU_DownLoop   
                            	L_RD_DownEnd:
0000AAB3 0C EE              	        jmp     L_RU_End 
                            	
                            	L_RU_NormalUp:
                            	L_RU_Loop:
0000AAB4 40 F0 0A AB        	        call    F_Delay 
0000AAB6 41 94              	        r2 = 0x0001 
0000AAB7 1A D5 12 70        	        [P_Watchdog_Clear] = r2 
0000AAB9 09 03 40 00        	        r1 += 0x40 
0000AABB 19 D3 17 70        	        [P_DAC1] = r1 
0000AABD 09 43 00 80        	        cmp     r1, 0x8000 
0000AABF 4C 4E              	        jne     L_RU_Loop 
                            	L_RU_End:
0000AAC0 90 90              			pop     r1,r2 from [sp] 
0000AAC1 90 9A              	  		retf 
                            	    	.ENDP
                            	    
