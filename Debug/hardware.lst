                            	F_SP_ReadQueue_NIC_A2000:
                            	F_SP_ReadQueue_NIC_S480:
                            	F_SP_ReadQueue_NIC_S240:
                            	F_SP_ReadQueue_NIC_MS01:
                            	F_SP_ReadQueue_NIC_DVR:
0000A999 12 95 A1 06        			r2 = [R_ReadIndex]
0000A99B 12 45 A2 06        			cmp r2,[R_WriteIndex]
0000A99D 03 5E              			je	L_RQ_QueueEmpty?
                            	
0000A99E 0A 05 11 06        			r2 += R_Queue				// get queue data index
0000A9A0 C2 92              			r1 = [r2]
                            	L_RQ_QueueEmpty?:
0000A9A1 90 9A              			RETF
                            			
                            	///////////////////////////////////////////
                            	// Function: Put a data to Queue
                            	// R1: Input
                            	// Destory: R1,R2
                            	///////////////////////////////////////////	
                            	F_SP_WriteQueue_A2000:
                            	F_SP_WriteQueue_S480:
                            	F_SP_WriteQueue_S240:
                            	F_SP_WriteQueue_MS01:
                            	F_SP_WriteQueue_DVR:
                            	F_SP_WriteQueue:
0000A9A2 12 95 A2 06        			r2 = [R_WriteIndex] 			// put data to queue
0000A9A4 0A 05 11 06        			r2 += R_Queue
0000A9A6 C2 D2              			[r2] = r1
                            	
0000A9A7 12 95 A2 06        			r2 = [R_WriteIndex]
0000A9A9 41 04              			r2 += 1
0000A9AA 0A 45 90 00        			cmp	r2, C_QueueSize
0000A9AC 01 4E              			jne	L_WQ_NotQueueBottom
0000A9AD 40 94              			r2 = 0
                            	L_WQ_NotQueueBottom:
0000A9AE 1A D5 A2 06        			[R_WriteIndex] = r2
0000A9B0 90 9A              			RETF
                            	
                            			
